{# templates/admin/dashboard.html.twig #}

{% extends '@EasyAdmin/layout.html.twig' %}
{% block content_title %}Administration du site
{% endblock %}
{% block body %}
	<div class="content">
		<h1></h1>
		<h2>Produits</h2>
		<ul>
			{% for product in products %}
				<li>{{ product.title }}
					-
					{{ product.price }}
					-
					{{ product.category }}</li>
			{% endfor %}
		</ul>
	</div>
{% endblock %}
